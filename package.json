{
  "name": "@hackworthltd/primer",
  "private": true,
  "engines": {
    "node": "^14.19.2",
    "pnpm": "^7.1.0"
  },
  "devDependencies": {
    "@typescript-eslint/parser": "^5.27.1",
    "eslint": "^8.17.0",
    "eslint-import-resolver-typescript": "^2.7.1",
    "eslint-plugin-import": "^2.26.0",
    "prettier": "^2.6.2",
    "typescript": "^4.7.3",
    "typescript-eslint-language-service": "^5.0.0",
    "typescript-language-server": "^0.11.0",
    "unified-language-server": "^2.1.0",
    "vite-plugin-optimize-persist": "^0.1.2",
    "vite-plugin-package-config": "^0.1.1",
    "vscode-css-languageserver-bin": "^1.4.0",
    "vscode-html-languageserver-bin": "^1.4.0",
    "vscode-json-languageserver": "^1.3.4",
    "yaml-language-server": "^1.8.0"
  },
  "scripts": {
    "format": "prettier --write .",
    "lint": "pnpm --parallel --stream -r lint",
    "lint:fix": "pnpm --parallel --stream -r lint:fix",
    "build": "pnpm -r build"
  },
  "pnpm": {
    "overrides": {
      "@hackworthltd/primer-types": "workspace:*",
      "@hackworthltd/primer-components": "workspace:*",
      "@hackworthltd/primer-app": "workspace:*"
    }
  }
}
